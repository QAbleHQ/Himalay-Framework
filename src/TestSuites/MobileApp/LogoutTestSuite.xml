<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Logout Page Android App Test Suite" parallel="tests" thread-count="5">

    <test name = "To verify user is able to logout by clicking on logout button">
        <classes>
            <class name = "MobileTests.Logout_Flow.Logout_test" >
                <methods>
                    <include name="To_verify_user_is_able_to_logout_by_clicking_on_logout_button"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "To verify user is able to logout by Taping on logout button">
        <classes>
            <class name = "MobileTests.Logout_Flow.Logout_test" >
                <methods>
                    <include name="To_verify_user_is_able_to_logout_by_Taping_on_logout_button"></include>
                </methods>
            </class>
        </classes>
    </test>

</suite>