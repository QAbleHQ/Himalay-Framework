<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "API_Regression">
    <test name = "getting started with valid token">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_valid_token"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with valid token with free plan">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_valid_token_with_free_plan"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with valid token application zip content type">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_valid_token_application_zip_content_type"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with invalid token">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_invalid_token"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with blank token">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_blank_token"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with title">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_title"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name = "getting started with blank title">
        <classes>
            <class name = "api.tests.GettingStartedSequenceTest">
                <methods>
                    <include name="getting_started_with_blank_title"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>