<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Login Page Android App Test Suite" parallel="tests" thread-count="5">

    <test name = "login_test">
        <classes>
            <class name = "MobileTests.login.LoginTests" >
                <methods>
                    <include name="login_test"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "verify registered user is trying to login by entering valid email and invalid password">
        <classes>
            <class name = "MobileTests.login.LoginTests" >
                <methods>
                    <include name="verify_registered_user_is_trying_to_login_by_entering_valid_email_and_invalid_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name = "verify registered user is trying to login by keeping email text field empty">
        <classes>
            <class name = "MobileTests.login.LoginTests" >
                <methods>
                    <include name="verify_registered_user_is_trying_to_login_by_keeping_email_text_field_empty"></include>
