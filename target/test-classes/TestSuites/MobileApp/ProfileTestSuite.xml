<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Profile Page Android App Test Suite" parallel="tests" thread-count="5">

    <test name="verify user is able to navigate back by clicking on back button">
        <classes>
            <class name="MobileTests.profile_tests.profile_test">
                <methods>
                    <include name="verify_user_is_able_to_navigate_back_by_clicking_on_back_button"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify that user is able to see profile details at profile option">
        <classes>
            <class name="MobileTests.profile_tests.profile_test">
                <methods>
                    <include name="verify_that_user_is_able_to_see_profile_details_at_profile_option"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="To verify user is able to see cancel and save button after editing anything in the profile details">
