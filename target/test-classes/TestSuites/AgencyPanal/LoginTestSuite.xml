<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Login Test Suite" parallel="tests" thread-count="5">

    <test name="Verify user is able to login with Valid credentials">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="Verify_login_with_valid_credentials"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with blank user">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_blank_user"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with blank password ">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_blank_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with user and password both empty">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_user_and_password_both_empty"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with valid user and invalid password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_valid_user_and_invalid_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login invalid user and valid password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_invalid_user_and_valid_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with wrong email format and password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_wrong_email_format_and_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with entering spaces in email and password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_entering_spaces_in_email_and_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with invalid single quote in email and password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_invalid_single_quote_in_email_and_password"></include>
                </methods>
            </class>
        </classes>
    </test>
    <test name="verify login with invalid user with html tag and password">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_login_with_invalid_user_with_html_tag_and_password"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="verify when click on Back to login link, it gets back to login page">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_when_click_on_Back_to_login_link_it_gets_back_to_login_page"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="To verify when Clicks on password eye icon for password Masked UnMasked">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="To_verify_when_Clicks_on_password_eye_icon_for_password_Masked_UnMasked"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="verify not getting reset password for unregistered user">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_not_getting_reset_password_for_unregistered_user"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name=" verify user gets mail when send reset password mail">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_user_gets_mail_when_send_reset_password_mail"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="verify when click on send button without entering email">
        <classes>
            <class name="WebUITests.login.LoginTests">
                <methods>
                    <include name="verify_when_click_on_send_button_without_entering_email"></include>
                </methods>
            </class>
        </classes>
    </test>


